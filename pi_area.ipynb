    "# print values for area of a circle for n in range(1,11):\n",
    "\n",
    "# import PI\n",
    "from math import pi\n",
    "\n",
    "even_areas = []\n",
    "for i in range(1,33):\n",
    "    print(i, round(pi * i**2, 2))\n",
    "    if int(pi * i**2) % 2 == 0:\n",
    "        even_areas.append(int(pi * i**2))\n",
    "\n",
    "print(even_areas)\n",
    "print(sum(even_areas))\n",
    "\n"
   ]
  }
